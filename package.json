{
  "name": "doodyide",
  "version": "1.0.0",
  "description": "DoodyIDE - an code edior with online compiler. Supports over 20+ languages",
  "main": "./out/main/index.js",
  "author": "https://github.com/myroslavmay",
  "homepage": "https://github.com/myroslavmay/doodyIDE",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:mac_icon": "npm run build && electron-builder --mac --icon ./build/icon.icns",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@monaco-editor/react": "^4.6.0",
    "axios": "^1.7.9",
    "electron-updater": "^6.1.7",
    "frame": "^1.0.0-alpha.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "next-themes": "^0.4.6",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vite": "^5.3.1"
  }
}
